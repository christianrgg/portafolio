overflow: hidden;\r\n    \r\n    &:before {\r\n        content: \"\";\r\n        position: absolute;\r\n        bottom: 0;\r\n        left: 0;\r\n        width: 100%;\r\n        height: 2px;\r\n        background: currentColor;\r\n        transform: scale3d(0, 1, 1);\r\n        transform-origin: left;\r\n        transition: transform 0.3s cubic-bezier(.5, .5, .3, 1);\r\n    }\r\n    &:after {\r\n        content: attr(data-text);\r\n        position: absolute;\r\n        top: 0;\r\n        height: 100%;\r\n        transform: translate3d(150%, 0, 0);\r\n        transition: transform 0.3s cubic-bezier(.5, .5, .3, 1);\r\n    }\r\n    & span {\r\n        transition: transform 0.3s cubic-bezier(.5, .5, .3, 1);\r\n    }\r\n    &:hover::before {\r\n        transform: scale3d(1, 1, 1);\r\n        transform-origin: left;\r\n    }\r\n    &:hover:after {\r\n        transform: translate3d(0, 0, 0);\r\n    }\r\n    &:hover span {\r\n        transform: translate3d(-150%, 0, 0);\r\n    }\r\n}\r\n.coolButton {\r\n    position: relative;\r\n    border: none;\r\n    cursor: pointer;\r\n    width: 175px;\r\n    height: 120px;\r\n    background: none;\r\n    color: #fff;\r\n    &:before,\r\n    &:after {\r\n        position: absolute;\r\n        top: 0;\r\n        left: 0;\r\n        width: 100%;\r\n        height: 100%;\r\n    }\r\n    &:before {\r\n        content: \"\";\r\n        background: #fff;\r\n        clip-path: path(\"M154.5,88.5 C131,113.5 62.5,110 30,89.5 C-2.5,69 -3.5,42 4.5,25.5 C12.5,9 33.5,-6 85,3.5 C136.5,13 178,63.5 154.5,88.5 Z\");\r\n        transition: clip-path 0.5s cubic-bezier(0.585, 2.5, 0.645, 0.55)\r\n    }\r\n    &:hover:before {\r\n        clip-path: path(\"M143,77 C117,96 74,100.5 45.5,91.5 C17,82.5 -10.5,57 5.5,31.5 C21.5,6 79,-5.5 130.5,4 C182,13.5 169,58 143,77 Z\");\r\n    }\r\n    & span {\r\n        mix-blend-mode: difference;\r\n        font-weight: 500;\r\n    }\r\n    &:after {\r\n        content:\"\";\r\n        height: 85%;\r\n        width: 95%;\r\n        border: 1px solid #fff;\r\n        top: 6px;\r\n        border-radius: 58% 42% 55% 45% / 56% 45% 55% 44%; \r\n        transform: rotate(-20deg);\r\n        transition: transform 0.5s cubic-bezier(0.585, 2.5, 0.645, 0.55)\r\n    }\r\n    &:hover:after{\r\n        transform: translate3d(0,-5px,0);\r\n    }\r\n    @media(max-width:800px) {\r\n        & {\r\n            transform: scale3d(0.7, 0.7, 0.7);\r\n            font-size: 23px;\r\n        }\r\n    }\r\n}\r\n.coloredIcon {\r\n    color: #fff;\r\n    font-size: 35px;\r\n    &:hover {\r\n        animation: coloredIconAnimation 0.2s infinite;\r\n    }\r\n    @keyframes coloredIconAnimation{\r\n        0% {\r\n            text-shadow: 5px 4px $red-color, -5px -6px $blue-color;\r\n        }\r\n        25% {\r\n            text-shadow: -5px -6px $red-color, 5px 4px $blue-color;\r\n        }\r\n        50% {\r\n            text-shadow: 5px -4px $red-color, -8px 4px $blue-color;\r\n        }\r\n        75% {\r\n            text-shadow: -8px -4px $red-color, -5px -4px $blue-color;\r\n        }\r\n        100% {\r\n            text-shadow: 5px 0 $red-color, 5px -4px $blue-color;\r\n        }\r\n        \r\n    }\r\n    // text-shadow: -10px -20px red, -15px -15px blue;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"99430455750b0ae791f8079f4ddad54d","cacheData":{"env":{}}}